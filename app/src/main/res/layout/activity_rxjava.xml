<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.rxjava.rxjavademo.ui.RxjavaActivity">

    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/rxjava"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rxjava第一种被观察者create"
        />
    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/rxjava2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rxjava第二种被观察者just"
        />
    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/rxjava3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rxjava第三种被观察者from"
        />
    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/rxjava5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rxjava由Subscriber创建观察者"
        />
    <Button
        android:layout_marginTop="10dp"
        android:id="@+id/rxjava4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rxjava线程调度器Scheduler "
        />

</LinearLayout>
